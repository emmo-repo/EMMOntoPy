@prefix : <http://emmo.info/mammal#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix animal: <http://emmo.info/animal#> .
@base <http://emmo.info/mammal> .

<http://emmo.info/animal> rdf:type owl:Ontology ;
    owl:versionIRI <http://emmo.info/mammal/0.1> ;
    owl:imports <http://emmo.info/animal/0.1> ;
    owl:versionInfo "0.1" .

:Mammal rdf:type owl:Class ;
    rdfs:subClassOf animal:Vertebrates ;
    animal:latinName "Mammalia"@en ;
    skos:prefLabel "Pattedyr"@no , "Mammal"@en .

:Rodent rdf:type owl:Class ;
    rdfs:subClassOf :Mammal ;
    skos:prefLabel "Gnager"@no , "Rodent"@en .

:Mouse rdf:type owl:Class ;
    rdfs:subClassOf :Rodent ;
    skos:prefLabel "Mouse"@en .

:Felines rdf:type owl:Class ;
    rdfs:subClassOf :Mammal ;
    skos:prefLabel "Kattedyr"@no , "Felines"@en .

:Cat rdf:type owl:Class ;
    rdf:comment "The below restrictions is not necessary true - not all cats chases mice."@en ;
    rdfs:subClassOf :Felines , [
        rdf:type owl:Restriction ;
        owl:onProperty :chasing ;
        owl:someValuesFrom :Mouse
    ] ;
    skos:prefLabel "Cat"@en .

:Jerry rdf:type :Mouse ;
    skos:prefLabel "Jerry"@en .

:Tom rdf:type :Cat ;
    animal:chasing :Jerry ;
    skos:prefLabel "Tom"@en .
