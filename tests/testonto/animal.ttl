@prefix : <http://emmo.info/animal#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <http://emmo.info/animal> .

<http://emmo.info/animal> rdf:type owl:Ontology ;
    owl:versionIRI <http://emmo.info/animal/0.1> ;
    owl:versionInfo "0.1" .

:latinName rdf:type owl:AnnotationProperty ;
    rdfs:subPropertyOf rdf:label ;
    skos:prefLabel "latinName"@en .

:hasLegs rdf:type owl:DatatypeProperty ;
    rdfs:domain :Animal ;
    rdfs:range xsd:int ;
    rdf:comment "Number of legs."@en ;
    skos:prefLabel "hasLegs"@en .

:chasing rdf:type owl:ObjectProperty ;
    rdfs:domain :Animal ;
    rdfs:range :Animal ;
    skos:prefLabel "chasing"@en .

:Animal rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    :latinName "Animalia"@en ;
    skos:prefLabel "Animal"@en .

:Vertebrates rdf:type owl:Class ;
    rdfs:subClassOf :Animal ;
    :latinName "Vertebrata"@en ;
    skos:prefLabel "Virveldyr"@no , "Vertebrates"@en .

:Insect rdf:type owl:Class ;
    rdfs:subClassOf :Animal , [
        rdf:type owl:Restriction ;
        owl:onProperty :hasLegs ;
        owl:hasValue "6"^^xsd:int
    ] ;
    :latinName "Insectum"@en ;
    skos:prefLabel "Insekt"@no , "Insect"@en .
